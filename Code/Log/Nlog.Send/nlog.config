<?xml version="1.0" encoding="utf-8" ?>
<nlog  xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   >
  <extensions>
    <add assembly="Nlog.Send"/>
  </extensions>
  <targets>


    <target name="cs" xsi:type="ColoredConsole"  encoding="utf-8">

      <layout xsi:type="CustomJsonLayout"/>
      
     
    </target>
    <target name="net" xsi:type="Network" address="udp://127.0.0.1:4001" encoding="utf-8">
      <layout xsi:type="CustomJsonLayout"/>
    </target>
  </targets>
  <rules>
    <logger name="*"  writeTo="net,cs" />
    <!--
      <logger name="*"   minlevel="Error" writeTo="file" />
 
      <logger name="*"  writeTo="ds" />
      -->
  </rules>
</nlog>